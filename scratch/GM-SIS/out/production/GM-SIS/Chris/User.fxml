<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<Pane minHeight="0.0" minWidth="0.0" prefHeight="507.0" prefWidth="897.0" xmlns="http://javafx.com/javafx/8.0.102" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Chris.UserController">
    <children>
        <TableView fx:id="userTable" layoutX="12.0" layoutY="85.0" prefHeight="339.0" prefWidth="877.0">
            <columns>
                <TableColumn fx:id="userID" prefWidth="183.0" text="UserID" />
                <TableColumn fx:id="UserName" prefWidth="117.0" text="Username" />
                <TableColumn fx:id="Password" prefWidth="237.0" text="Password" />
                <TableColumn fx:id="adminStatus" prefWidth="165.0" text="Admin Status" />
            </columns>
        </TableView>
        <Text layoutX="22.0" layoutY="58.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Users">
            <font>
                <Font name="Cambria" size="36.0" />
            </font>
        </Text>
        <Button fx:id="addUser" layoutX="22.0" layoutY="459.0" mnemonicParsing="false" onAction="#add" text="Add User" />
        <Button fx:id="editUser" layoutX="90.0" layoutY="459.0" mnemonicParsing="false" onAction="#edit" text="Edit" />
        <Button fx:id="deleteUserbtn" layoutX="136.0" layoutY="459.0" mnemonicParsing="false" onAction="#delete" text="Delete" />
        <Button fx:id="logOut" layoutX="274.0" layoutY="459.0" mnemonicParsing="false" text="Log out" />
      <Button fx:id="refresh" layoutX="196.0" layoutY="459.0" mnemonicParsing="false" onAction="#fillTable" text="Refresh" />
    </children>
</Pane>
