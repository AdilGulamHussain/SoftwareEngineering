<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Accordion?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="300" prefWidth="275" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Chris.LoginController">
   <children>
      <TextField fx:id="txt_username" layoutX="87.0" layoutY="80.0" promptText="username" />
      <PasswordField fx:id="txt_password" onAction="#login" layoutX="87.0" layoutY="118.0" promptText="password" />
      <Button fx:id="button" layoutX="137.0" layoutY="160.0" mnemonicParsing="false" onAction="#login" text="Login" />
      <Label fx:id="message" alignment="CENTER" contentDisplay="CENTER" layoutX="-1.0" layoutY="194.0" prefHeight="17.0" prefWidth="320.0" textAlignment="CENTER">
         <font>
            <Font name="Agency FB" size="24.0" />
         </font></Label>
      <Accordion layoutX="34.0" layoutY="-11.0" />
      <Label layoutX="87.0" layoutY="14.0" prefHeight="66.0" prefWidth="208.0" text="Welcome " textAlignment="CENTER">
         <font>
            <Font name="Agency FB" size="45.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
